<h2>Your Dashboard</h2>
<div class="banner-block">
  <a class="btn btn-default banner-block-button" href="/duckify">go duckify stuff</a>
</div>
<div class="gallery">
  <div class="columns">
    <% duckfaces.forEach(function(duckface){ %>
      <div class="gallery-thumbnail">
        <a href="/dashboard/<%= duckface.id %>"><img src="<%- duckface.src %>"></a>
      </div>
    <% }); %>
  </div>
</div>
